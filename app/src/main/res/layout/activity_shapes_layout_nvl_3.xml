        tools:srcCompat="@drawable/ic_estrella_celeste" />

    <ImageView
        android:id="@+id/ivNVL3Drag4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        android:tag="3"
        app:layout_constraintBottom_toBottomOf="@+id/ivNVL3Drag3"
        app:layout_constraintEnd_toStartOf="@+id/ivNVL3Drag5"
        app:layout_constraintStart_toEndOf="@+id/ivNVL3Drag3"
        app:srcCompat="@drawable/ic_hex_rosa"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/ivShelf1Shapes3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:background="#FFFFFF"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/ivShelf3Shapes3"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivShelf2Shapes3"
        app:layout_constraintVertical_bias="0.07"
        app:srcCompat="@drawable/ic_escalon"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/ivNVL3Drag1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        android:tag="0"
        app:layout_constraintBottom_toBottomOf="@+id/ivNVL3Drag2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_cuadrado_amarillo" />

    <ImageView
        android:id="@+id/ivNVL3Drag5"
