        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/ivLife1SH3"
        app:layout_constraintTop_toTopOf="@+id/ivLife1SH3"
