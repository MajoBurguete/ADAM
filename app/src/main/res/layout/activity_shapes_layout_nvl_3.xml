        app:layout_constraintEnd_toEndOf="@+id/ivShelf2Shapes3"
        app:layout_constraintStart_toStartOf="@+id/ivShelf2Shapes3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_hex_rosa" />

    <ImageView
        android:id="@+id/ivNVL3Figure5"
        android:layout_width="wrap_content"
