        app:layout_constraintEnd_toEndOf="@+id/ivShelf1Shapes3"
        app:layout_constraintStart_toStartOf="@+id/ivShelf1Shapes3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_triangulo_naranja" />

    <TextView
        android:id="@+id/tvScoreShapesNVL3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/silka_bold_webfont"
        android:text="Puntuación: 0"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvShapesNVL3"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/ivLife1SH3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/ibPauseShapes3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ibPauseShapes3"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_adam_alive" />

    <ImageView
