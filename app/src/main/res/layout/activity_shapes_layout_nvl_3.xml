        app:layout_constraintEnd_toEndOf="@+id/ivShelf5Shapes3"
        app:layout_constraintStart_toStartOf="@+id/ivShelf5Shapes3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_cuadrado_amarillo" />

    <ImageView
        android:id="@+id/ivNVL3Figure2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-10dp"
