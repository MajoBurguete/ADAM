        android:background="#FFFFFF"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ivShelf5Shapes3"
        app:layout_constraintTop_toBottomOf="@+id/ivShelf3Shapes3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_escalon"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/ivNVL3Drag2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        android:tag="1"
        app:layout_constraintBottom_toBottomOf="@+id/ivNVL3Drag3"
        app:layout_constraintEnd_toStartOf="@+id/ivNVL3Drag3"
        app:layout_constraintStart_toEndOf="@+id/ivNVL3Drag1"
