<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/verde_palido_dos"
    tools:context=".Activities.ShapesActivity">

    <ImageButton
        android:id="@+id/ibPauseShapes1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.951"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.02"
        app:srcCompat="@drawable/ic_pausa"
        tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck" />

    <ImageView
        android:id="@+id/ivShelf3Shapes1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ibPauseShapes1"
        app:layout_constraintVertical_bias="0.10"
        app:srcCompat="@drawable/ic_escalon"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/ivShelf2Shapes1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ivShelf3Shapes1"
        app:layout_constraintTop_toBottomOf="@+id/ivShelf3Shapes1"
        app:layout_constraintVertical_bias="0.14"
        app:srcCompat="@drawable/ic_escalon"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/ivShelf1Shapes1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ivShelf2Shapes1"
        app:layout_constraintTop_toBottomOf="@+id/ivShelf2Shapes1"
        app:layout_constraintVertical_bias="0.2"
        app:srcCompat="@drawable/ic_escalon"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/ivNVL1Drag2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="15dp"
        android:backgroundTint="@android:color/transparent"
        android:tag="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_cuadrado_amarillo" />

    <ImageView
        android:id="@+id/ivNVL1Drag1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:backgroundTint="@android:color/transparent"
        android:tag="0"
        app:layout_constraintBottom_toBottomOf="@+id/ivNVL1Drag2"
        app:layout_constraintEnd_toStartOf="@+id/ivNVL1Drag2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ivNVL1Drag2"
        app:srcCompat="@drawable/ic_estrella_celeste"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/ivNVL1Drag3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:backgroundTint="@android:color/transparent"
        android:tag="2"
        app:layout_constraintBottom_toBottomOf="@+id/ivNVL1Drag2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.993"
        app:layout_constraintStart_toEndOf="@+id/ivNVL1Drag2"
        app:layout_constraintTop_toTopOf="@+id/ivNVL1Drag2"
        app:srcCompat="@drawable/ic_hex_rosa"
        tools:ignore="ImageContrastCheck" />

    <TextView
        android:id="@+id/tvShapesNVL1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/silka_bold_webfont"
        android:text="Â¡Ve las posiciones!"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="26sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivShelf1Shapes1"
        app:layout_constraintVertical_bias="0.04"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/btnContinueNVL1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/r_rosa2"
        android:fontFamily="@font/silka_medium_webfont"
        android:text="Seguir"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/ivNVL1Drag2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvScoreShapesNVL1"
        app:layout_constraintVertical_bias="0.05" />

    <Button
        android:id="@+id/btnRestartNVL1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:backgroundTint="@color/r_azul"
        android:fontFamily="@font/silka_medium_webfont"
        android:text="Limpiar"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/btnContinueNVL1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/btnContinueNVL1"
        app:layout_constraintTop_toTopOf="@+id/btnContinueNVL1" />

    <Button
        android:id="@+id/btnCheckNVL1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:backgroundTint="@color/r_naranja"
        android:fontFamily="@font/silka_medium_webfont"
        android:text="Checar"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/btnContinueNVL1"
        app:layout_constraintEnd_toStartOf="@+id/btnContinueNVL1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btnContinueNVL1" />

    <ImageView
        android:id="@+id/ivNVL1Figure1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-10dp"
        app:layout_constraintBottom_toTopOf="@+id/ivShelf3Shapes1"
        app:layout_constraintEnd_toEndOf="@+id/ivShelf3Shapes1"
        app:layout_constraintStart_toStartOf="@+id/ivShelf3Shapes1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_estrella_celeste" />

    <ImageView
        android:id="@+id/ivNVL1Figure2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-10dp"
        app:layout_constraintBottom_toTopOf="@+id/ivShelf2Shapes1"
        app:layout_constraintEnd_toEndOf="@+id/ivShelf2Shapes1"
        app:layout_constraintStart_toStartOf="@+id/ivShelf2Shapes1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_cuadrado_amarillo" />

    <ImageView
        android:id="@+id/ivNVL1Figure3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-10dp"
        app:layout_constraintBottom_toTopOf="@+id/ivShelf1Shapes1"
        app:layout_constraintEnd_toEndOf="@+id/ivShelf1Shapes1"
        app:layout_constraintStart_toStartOf="@+id/ivShelf1Shapes1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_hex_rosa" />

    <ImageView
        android:id="@+id/ivLife1SH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/ibPauseShapes1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ibPauseShapes1"
        app:srcCompat="@drawable/ic_adam_alive" />

    <ImageView
        android:id="@+id/ivLife2SH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/ivLife1SH"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/ivLife1SH"
        app:layout_constraintTop_toTopOf="@+id/ivLife1SH"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_adam_alive" />

    <ImageView
        android:id="@+id/ivLife3SH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/ivLife2SH"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/ivLife2SH"
        app:layout_constraintTop_toTopOf="@+id/ivLife2SH"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_adam_alive" />

    <TextView
        android:id="@+id/tvScoreShapesNVL1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/silka_medium_webfont"
        android:text="Puntaje: 0"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvShapesNVL1"
        app:layout_constraintVertical_bias="0.05" />

    <FrameLayout
        android:id="@+id/flPauseShapes1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:clickable="false"
        android:translationZ="3dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>